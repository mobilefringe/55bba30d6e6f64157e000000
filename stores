<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var stores = getStoresList();
            renderStoreList('#store_list_container','#store_list_template', stores, "stores");
            
            var categories = getStoreCategories();
            var category_list = [];
            $.each( categories, function( key, val ){
                if (val.store_ids != null) {
                    category_list.push(val);
                } 
            });
            // var all_cat = {};
            // all_cat.name = "All Categories";
            // all_cat.id= 0;
            // category_list.unshift(all_cat);
            // renderGeneral('#category_select_container', '#category_select_template', category_list);
            init_side();
            show_content();
        }
        loadMallDataCached(renderAll); 
    })
</script>